{
  "name": "DS Automator",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "./icon.png",
  "description": "Automation of design system layouts",
  "authorEmail": "npi@bk.com",
  "author": "Pavel Neznanov",
  "commands": [
    {
      "script": "pages.js",
      "handler": "pages",
      "shortcut": "command alt shift p",
      "name": "Format pages",
      "identifier": "ds-plugin-pages",
      "icon": "./icons/icon-sr.png",
      "description": "Sort and format the page list"
    },
    {
      "script": "clean-pages.js",
      "handler": "cleanPages",
      "shortcut": "command alt shift c",
      "name": "Clear page numbers",
      "identifier": "ds-plugin-clean-pages",
      "icon": "./icons/icon-sr.png",
      "description": "Removing prefixes in page titles"
    },
    {
      "script": "artboards.js",
      "handler": "artboards",
      "name": "Format artboard titles",
      "identifier": "ds-plugin-artboards",
      "description": "Format artboard titles in document"
    }
  ],
  "version": "1.1.0",
  "identifier": "ds-automator-sketch-plugin",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/pavelneznanov/ds-automator-sketch-plugin/master/.appcast.xml"
}